{
	"name": "nuxt-app",
	"private": true,
	"prisma": {
		"seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
	},
	"scripts": {
		"build": "nuxt build",
		"dev": "nuxt dev",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"sb:stop": "npx env-cmd npx supabase stop --backup",
		"sb:start": "npx env-cmd npx supabase start",
		"sb:types": "npx supabase gen types typescript --local tb > db_types.ts",
		"prisma:reset-rls": "npx prisma db execute --file ./prisma/sql/reset-rls.sql --schema ./prisma/schema.prisma"
	},
	"devDependencies": {
		"@nuxt/devtools": "^0.4.1",
		"@nuxtjs/supabase": "^0.3.5",
		"@nuxtjs/tailwindcss": "^6.6.6",
		"@prisma/client": "^4.13.0",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.9",
		"@types/node": "^18.15.12",
		"env-cmd": "^10.1.0",
		"nuxt": "^3.4.2",
		"prisma": "^4.13.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4"
	},
	"dependencies": {
		"@pinia/nuxt": "^0.4.9",
		"stripe": "^12.1.1",
		"zod": "^3.21.4"
	}
}
